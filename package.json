{
  "name": "errok",
  "version": "1.0.0",
  "description": "Type-safe error handling",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest && npm run test-types",
    "test-types": "tsc --noEmit -p ./tests/tsconfig.tests.json",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier --write 'src/**/*.ts?(x)' && npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf ./dist ./tmp",
    "build": "npm run clean && rollup --config && mv tmp/*.js dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tunnel-Labs/errok.git"
  },
  "author": "Giorgio Delgado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tunnel-Labs/errok/issues"
  },
  "homepage": "https://github.com/Tunnel-Labs/errok#readme",
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.8",
    "@babel/preset-typescript": "7.23.3",
    "@tsconfig/node14": "^14.1.0",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.0",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "babel-jest": "29.7.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "rollup": "4.9.4",
    "rollup-plugin-dts": "6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "testdouble": "3.20.1",
    "ts-jest": "29.1.1",
    "ts-toolbelt": "9.6.0",
    "tslib": "^2.6.2",
    "typescript": "5.3.3"
  },
  "keywords": [
    "typescript",
    "functional",
    "fp",
    "error"
  ]
}
